<?xml version="1.0" encoding="UTF-8"?>

<assembly>
	<id>gviewer</id>
	<formats>
		<format>zip</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<fileSets>
		<fileSet>
			<directory>src/main/webapp</directory>
			<excludes>
				<exclude>html/GeneMap/node_modules/**</exclude>
				<exclude>html/GeneMap/bower_components/**</exclude>
			</excludes>
			<outputDirectory>/</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>src/main/webapp/html/GeneMap/assets/</directory>
			<outputDirectory>/</outputDirectory>
		</fileSet>
	</fileSets>
</assembly>
