<urlrewrite>
	<!-- Configures UrlRewriteFilter, see the POM -->
	<rule>
	  <from>.*</from>
	  <!-- CORS rule, instruct the web browser to accept Javascript-originated requests to the data server -->
	  <set type="response-header" name="Access-Control-Allow-Origin">${knetminer.api.base_url}</set>
  </rule>
</urlrewrite>
